<h1>Listing posts</h1>

<table>
  <tr>
    <th>Twitterid</th>
    <th>Content</th>
    <th>Handled</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.twitterid %></td>
    <td><%= post.content %></td>
    <td><%= post.handled %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
